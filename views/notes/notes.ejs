    <% notes.forEach((note) => { %>
<div class="mb-4" id="note-<%= note.id %>">
<div class="w-auto p-6 bg-white border border-gray-200 rounded-lg shadow dark:bg-gray-800 dark:border-gray-700">
    <!-- <a href="#"> -->
    <!--     <h5 class="mb-2 text-2xl font-bold tracking-tight text-gray-900 dark:text-white">Noteworthy technology acquisitions 2021</h5> -->
    <!-- </a> -->
    <p class="mb-3 font-normal text-gray-700 mb-4 dark:text-gray-400"><%- note.body %></p>
    <br />
    <button hx-swap="delete" hx-confirm="Are you sure you want to delete this note?" hx-target="#note-<%= note.id %>" hx-post="/notes/delete/<%= note.id %>" class="inline-flex items-center px-3 py-2 text-sm font-medium text-center text-white bg-red-700 rounded-lg hover:bg-red-800 focus:ring-4 focus:outline-none focus:ring-red-300 dark:bg-red-600 dark:hover:bg-red-700 dark:focus:ring-red-800">
        Delete 
        <svg class="rtl:rotate-180 w-3.5 h-3.5 ms-2" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 14 10">
            <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M1 5h12m0 0L9 1m4 4L9 9"/>
        </svg>
    </button>
</div>

</div>
<% }) %>
